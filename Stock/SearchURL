  <script>
    jQuery( "#usfSecurityListToggle" ).click(function() {
      jQuery( "#usfSecurityListCategoryBlock" ).toggle();//Kategorieliste ein bzw. ausblenden
      jQuery( "#usfSecurityListCategoryBlock_index_germany" ).click();
      if(jQuery( "#usfSecurityListCategoryBlock" ).is(':visible')) {
        jQuery( "#usfSecurityListToggleText" ).text('Charts ausblenden');
      } else {
        jQuery( "#usfSecurityListToggleText" ).text('Charts einblenden');
      }
    });
    jQuery( ".usfSecurityListCategory" ).click(function() {
      //console.log(jQuery(this).attr('id'),jQuery(this).data('list-id'));
      jQuery('#usfSecurityListCategoryDetails').USFajaxRequest({'par': 'snippet=kursleisteKategorieDetails&listId='+jQuery(this).data('list-id'),'filename':'ajax/get_kursleiste.htn','rootDir':'/ard/'});
      jQuery( ".usfSecurityListCategory" ).removeClass('active');
      jQuery( this ).addClass('active');
    });
  </script>
  
